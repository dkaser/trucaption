{
  "name": "trucaption",
  "version": "1.3.0",
  "description": "Computer-aided realtime transcription",
  "main": "src/server.js",
  "scripts": {
    "start": "electron-forge start",
    "electron": "electron .",
    "package": "electron-forge package",
    "package:darwin-arm64": "electron-forge package --arch=arm64 --platform=darwin",
    "package:darwin-x64": "electron-forge package --arch=x64 --platform=darwin",
    "make": "electron-forge make",
    "make-win": "electron-forge make --arch=x64 --platform=win32"
  },
  "keywords": [
    "captions",
    "transcript",
    "cart"
  ],
  "author": "Derek Kaser",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@trucaption/common": "workspace:^",
    "axios": "^0.27.2",
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "ip": "^1.1.8",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.1",
    "@electron-forge/maker-deb": "^6.4.1",
    "@electron-forge/maker-rpm": "^6.4.1",
    "@electron-forge/maker-squirrel": "^6.4.1",
    "@electron-forge/maker-zip": "^6.4.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.1",
    "cross-conf-env": "^1.3.0",
    "electron": "26.1.0"
