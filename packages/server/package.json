{
  "name": "@trucaption/trucaption",
  "version": "1.2.0",
  "description": "Computer-aided realtime transcription",
  "bin": "dist/index.js",
  "scripts": {
    "build": "npm run check && ncc build ./src/server.js -o dist",
    "start": "node dist/index.js",
    "test": "node src/server.js",
    "format": "npx rome format --write .",
    "check": "npx rome check .",
    "package": "pkg . -C Brotli --no-bytecode"
  },
  "keywords": [
    "captions",
    "transcript",
    "cart"
  ],
  "author": "Derek Kaser",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "ip": "^1.1.8",
    "open": "^8.4.2",
    "socket.io": "^4.7.2"
  },
  "pkg": {
    "targets": [
      "node18-win-x64",
      "node18-macos-x64",
      "node18-linuxstatic-x64",
      "node18-win-arm64",
      "node18-macos-arm64",
      "node18-linuxstatic-arm64"
    ],
    "assets": [
      "dist/**/*",
      "dist/xdg-open"
    ],
    "outputPath": "../../dist"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "pkg": "^5.8.1"
  }
}
